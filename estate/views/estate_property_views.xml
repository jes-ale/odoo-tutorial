<?xml version="1.0"?>
<odoo>
    <record id="estate_model_action" model="ir.actions.act_window">
        <field name="name">Estate action</field>
        <field name="res_model">estate_property</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Tutorial component
            </p>
        </field>
    </record>

    <record id="estate_poperty_view_tree" model="ir.ui.view">
        <field name="name">estate_property_tree</field>
        <field name="model">estate_property</field>
        <field name="arch" type="xml">
            <tree string="Propiedad">
                <field name="name">Title</field>
                <field name="postcode">Postcode</field>
                <field name="bedrooms">Bedrooms</field>
                <field name="living_area">Living Area (sqm)</field>
                <field name="expected_price">Expected Price</field>
                <field name="selling_price">Selling Price</field>
                <field name="available">Available From</field>
            </tree>
        </field>
    </record>
</odoo>